<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="IconStyle">
        <item name="android:layout_width">@dimen/_35sdp</item>
        <item name="android:layout_height">@dimen/_35sdp</item>
        <item name="android:padding">@dimen/_8sdp</item>
    </style>

    <style name="TextStyle">
        <item name="android:textSize">@dimen/_14sdp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">@font/font</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="CustomRatingBar" parent="Widget.AppCompat.RatingBar">
        <item name="android:progressDrawable">@drawable/custom_star</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:maxHeight">48dp</item>
    </style>

</resources>